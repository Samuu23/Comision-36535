<h2 style="color:crimson;"> History</h2>

<% const arePeople = peoples.length > 0; %>
<% if(arePeople){ %>
<div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Name</th>
            <th>Last Name</th>
            <th>Age</th>
        </tr>
        <% for (const people of peoples) {%>
        <tr>
            <td>
                <%=people.name%>
            </td>
            <td>
                <%=people.lastName%>
            </td>
            <td>
                <%=people.age%>
            </td>
        </tr>
        <%};%>
    </table>
</div>
<% } else{ %>
<h3 class="alert alert-danger"> No data</h1>
    <% } %>
